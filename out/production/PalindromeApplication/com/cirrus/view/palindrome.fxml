<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="LoginLayout" prefHeight="333.0" prefWidth="548.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cirrus.controller.PalindromeController">

    <children>

        <Label fx:id="lblLowerBoundary" layoutX="101.0" layoutY="117.0" prefHeight="17.0" prefWidth="108.0" text="Lower Bound:" textFill="#187c8a">
            <font>
                <Font name="Arial" size="15.0" />
            </font></Label>
        <TextField fx:id="lower" layoutX="296.0" layoutY="110.0" prefHeight="25.0" prefWidth="168.0" promptText="Enter Lower Boundary" />
        <Label fx:id="lblUpperBoundary" layoutX="101.0" layoutY="159.0" prefHeight="17.0" prefWidth="108.0" text="Upper Bound:" textFill="#358380">
            <font>
                <Font size="15.0" />
            </font></Label>
        <TextField fx:id="higher" layoutX="296.0" layoutY="154.0" prefHeight="25.0" prefWidth="168.0" promptText="Enter Upper Boundary" />
        <Button fx:id="btnLogin" layoutX="328.0" layoutY="215.0" mnemonicParsing="false" onAction="#search" prefHeight="25.0" prefWidth="108.0" text="Search" />
        <Label fx:id="lblTitle" layoutX="119.0" layoutY="27.0" prefHeight="20.0" prefWidth="329.0" text="Palindrome Calculation Application" textFill="#358385">
